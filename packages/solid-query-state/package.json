{
    "name": "solid-query-state",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/ItsNoxius/solid-query-state.git"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsup",
        "prepublishOnly": "pnpm run build",
        "test": "vitest",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "typecheck": "tsc --noEmit",
        "prepack": "bash scripts/prepack.sh"
    },
    "devDependencies": {
        "@solidjs/testing-library": "^0.8.10",
        "@vitest/coverage-v8": "^4.0.18",
        "jsdom": "^28.1.0",
        "solid-js": "^1",
        "tsup": "^8.5.1",
        "vite-plugin-solid": "^2.11.10",
        "vitest": "^4.0.18"
    },
    "peerDependencies": {
        "solid-js": "^1",
        "typescript": "^5"
    }
}